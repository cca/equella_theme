  },
  "homepage": "https://github.com/cca/equella_theme",
  "devDependencies": {
    "grunt": "^1.6.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-open": "^0.2.4",
    "grunt-sass": "^4.0.0",
    "load-grunt-tasks": "^5.1.0",
    "sass": "^1.86.3",
